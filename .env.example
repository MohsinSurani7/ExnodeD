# ExNode Environment Variables
# Copy this file to .env and fill in your API keys

# YouTube API (optional - for enhanced metadata)
YOUTUBE_API_KEY=your_youtube_api_key_here

# Instagram API (if using official API)
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here

# TikTok API (if available)
TIKTOK_API_KEY=your_tiktok_api_key_here

# Facebook API (if using official API)
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token_here

# Twitter API
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Vimeo API
VIMEO_ACCESS_TOKEN=your_vimeo_access_token_here

# Firebase Analytics (optional)
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_PROJECT_ID=your_firebase_project_id_here

# AdMob (optional)
ADMOB_APP_ID=your_admob_app_id_here
ADMOB_BANNER_UNIT_ID=your_admob_banner_unit_id_here
ADMOB_INTERSTITIAL_UNIT_ID=your_admob_interstitial_unit_id_here

# App Configuration
APP_ENV=development
DEBUG_MODE=true
ANALYTICS_ENABLED=false
ADS_ENABLED=false

